{
  "manifest_version": 2,

  "name": "I don't fucking care!",
  "description": "Filter web-pages by keywords. Keywords are set per domain.",
  "version": "0.0.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "I don't fucking care!"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["idontfuckingcare.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
